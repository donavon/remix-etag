{
  "version": "0.2.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "remix-etag",
  "author": "Donavon West <github@donavon.com> (https://donavon.com/)",
  "module": "dist/remix-etag.esm.js",
  "size-limit": [
    {
      "path": "dist/remix-etag.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/remix-etag.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@remix-run/node": "^1.3.2",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/etag": "^1.8.1",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "etag": "^1.8.1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
